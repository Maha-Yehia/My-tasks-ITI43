@model IEnumerable<ITIDbEFCorePowerTool.Models.Instructor>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Ins_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Ins_Degree)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Salary)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dept_)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Ins_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ins_Degree)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Salary)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dept_.Dept_Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Ins_Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Ins_Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Ins_Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
